{
  "name": "context-aware-writing-assistant",
  "version": "1.0.0",
  "description": "Context-aware writing assistant with real-time suggestions",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "deploy": "npm run build && vercel --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mromanoff/context-aware-writing-assistant.git"
  },
  "keywords": [
    "writing-assistant",
    "ai",
    "openai",
    "react",
    "typescript",
    "text-analysis",
    "readability"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mromanoff/context-aware-writing-assistant/issues"
  },
  "homepage": "https://github.com/mromanoff/context-aware-writing-assistant#readme",
  "dependencies": {
    "openai": "^6.9.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "jsdom": "^27.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
  }
